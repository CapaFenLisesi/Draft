  3.6662497787306404`*^9, 3.666249820368494*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"P", ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"KroneckerProduct", "[", 
      RowBox[{
       RowBox[{"PauliMatrix", "[", "i", "]"}], ",", 
       RowBox[{"PauliMatrix", "[", "j", "]"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "4"}], "}"}], ",", 
